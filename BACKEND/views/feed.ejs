                    <img src="img/svg/gray-white.svg" alt="img">
                    <img src="<%= USER.profilePic %>" alt="profile">
                    <div class="info">
                        <h2>
                            <%= USER.name %>
                        </h2>
                        <p class="title">
                            <%= USER.headline %>
                        </p>
                        <p class="address">
                            <%= USER.location %>
                        </p>

                    </div>
                </div>
                <div class="card left-sec-card">
                    <p>Connections</p>
                    <p>
                        <%= USER.connections %>
                    </p>
                    <p>Conncet with alumni</p>

                </div>
                <div class="left-sec-card-2">
                    <p>Acheice 4x more profile visits</p>
                    <p>
                        <img src="img/so_called_prem.png" alt="prem">
                        Try Premium for PKR 0
                    </p>
                </div>
                <div class="card left-last-card">
